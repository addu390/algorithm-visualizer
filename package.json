{
  "name": "dsaw",
  "version": "1.0.0",
  "description": "Demo Quad and KS tree",
  "scripts": {
    "clean": "rm -rf .parcel-cache && rm -rf dist",
    "build": "npm run build:index && npm run build:quadtree",
    "build:index": "parcel build index.html --dist-dir dist --public-url .",
    "build:quadtrees": "parcel build quadtree/demo/*.html --dist-dir dist/quadtree/demo --public-url .",
    "deploy": "npm run clean && npm run build && gh-pages -d dist",
    "dev": "parcel index.html"
  },
  "dependencies": {
    "d3": "^7.1.1",
    "d3-quadtree": "^3.0.1",
    "d3-scale": "^4.0.0",
    "get-pixels": "^3.3.3"
  },
  "devDependencies": {
    "parcel": "^2.0.1",
    "gh-pages": "^2.2.0"
  }
}
